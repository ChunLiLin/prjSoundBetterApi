@model IEnumerable<prjSoundBetterApi.Models.TProject>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FBudget)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FStartdate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FEnddate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FThumbnailPath)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FMember)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FProjectStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FSite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FSiteNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FBudget)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FStartdate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FEnddate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FThumbnailPath)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FMember.FMemberId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FProjectStatus.FProjectStatusId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FSite.FSiteId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FSiteNavigation.FStyleId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FProjectId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FProjectId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FProjectId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
